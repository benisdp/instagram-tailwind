    "@emotion/styled": "^11.6.0",
    "@glidejs/glide": "^3.5.2",
    "@mui/icons-material": "^5.2.4",
    "@mui/material": "^5.2.4",
    "@mui/styles": "^5.7.0",
    "@mui/system": "^5.2.4",
    "@shopify/app-bridge": "^2.0.22",
    "@shopify/app-bridge-react": "^2.0.26",
    "@shopify/app-bridge-utils": "^2.0.26",
    "@shopify/polaris": "^9.2.2",
    "@shopify/shopify-api": "^3.0.0",
    "@vitejs/plugin-react": "1.2.0",
    "axios": "^0.26.0",
    "bootstrap": "^5.1.3",
    "compression": "^1.7.4",
    "cookie-parser": "^1.4.6",
    "cross-env": "^7.0.3",
    "date-fns": "^2.28.0",
    "dotenv": "^16.0.0",
    "express": "^4.17.3",
    "graphql": "^16.3.0",
    "micromodal": "^0.4.10",
    "mongoose": "^6.2.3",
    "morgan": "^1.10.0",
    "qs": "^6.10.3",
    "react": "^17.0.2",
    "react-color": "^2.19.3",
    "react-dom": "^17.0.2",
    "react-helmet": "^6.1.0",
    "react-loading-overlay": "^1.0.1",
    "serve-static": "^1.14.1",
    "uniqid": "^5.4.0",
    "vite": "^2.9.8"
  },
  "devDependencies": {
    "autoprefixer": "^10.4.7",
    "husky": "^7.0.4",
    "nodemon": "^1.3.3",
